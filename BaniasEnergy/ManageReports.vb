    Private Sub Button5_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button5.Click, Button4.Click, Button3.Click, Button2.Click, Button1.Click, Button22.Click
        MysalaryReports = New SalaryReports
        Select Case sender.name
            Case Button1.Name
                MysalaryReports.TextBox1.Text = Button1.Name
            Case Button2.Name
                MysalaryReports.TextBox1.Text = Button2.Name
            Case Button3.Name
                MysalaryReports.TextBox1.Text = Button3.Name
            Case Button4.Name
                MysalaryReports.TextBox1.Text = Button4.Name
            Case Button5.Name
                MysalaryReports.TextBox1.Text = Button5.Name
            Case Button22.Name
                MysalaryReports.TextBox1.Text = Button22.Name
        End Select
        MysalaryReports.Show()
    End Sub

